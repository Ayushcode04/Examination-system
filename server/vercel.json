{
    "builds": [
        {
            "src": "exam_sys/asgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "exam_sys/asgi.py"
        }
    ]
}